{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "/src/browser/src/main.js",
  "scripts": {
    "dev": "gulp serve & npm run watch",
    "start": "electron src/browser/src/main.js",
    "watch:js": "babel --watch -d src/renderer/dist/ src/renderer/src/",
    "build:js": "browserify --extension=js -o src/renderer/dist/bundle.js src/renderer/src/index.js",
    "build:lib": "browserify --extension=js -o lib/index.js src/index.js",
    "watch": "npm run watch:js & npm run watch:stylus",
    "watch:stylus": "stylus src/renderer/stylus/ --watch  --out src/renderer/stylesheets/",
    "build:stylus": "stylus src/renderer/stylus/ --out src/renderer/stylesheets/",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0",
    "babel-plugin-syntax-do-expressions": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-do-expressions": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "brfs": "^1.4.2",
    "browserify": "^12.0.1",
    "electron-connect": "^0.3.3",
    "electron-prebuilt": "^0.36.5",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "gulp": "^3.9.0",
    "stylus": "^0.53.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "electron-prebuilt": "^0.37.2",
    "jsonfile": "^2.2.3",
    "lodash": "^3.10.1",
    "node-twitter-api": "^1.6.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-infinite": "^0.7.3",
    "react-redux": "^4.0.3",
    "react-sortable-pane": "^0.2.2",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "twitter": "^1.2.5"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
